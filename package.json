{
  "name": "nuxt-parse-next",
  "version": "0.2.0",
  "description": "A parse module for Nuxt.js",
  "author": "Vittorio Emmermann",
  "keywords": [
    "vue",
    "nuxt",
    "nuxt.js"
  ],
  "main": "lib/module.js",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt test/fixture",
    "test": "jest",
    "test:unit": "jest --testRegex \"/test/unit/(.+)\\.test\\.js$\"",
    "test:system": "jest --testRegex \"/test/system/(.+)\\.test\\.js$\"",
    "test:coverage": "yarn test --coverage",
    "format": "prettier './**/*.{js,ts,json,vue}' --write"
  },
  "devDependencies": {
    "jest": "25.1.0",
    "nuxt": "2.13.0",
    "prettier": "1.19.1",
    "puppeteer": "2.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "consola": "^2.3.2",
    "parse": "^2.14.0"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laplaceliu/nuxt-parse-next.git"
  },
  "bugs": {
    "url": "https://github.com/laplaceliu/nuxt-parse-next/issues"
  },
  "homepage": "https://github.com/laplaceliu/nuxt-parse-next#readme"
}
